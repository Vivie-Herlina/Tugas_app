<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="@drawable/gunung"
    android:paddingRight="0dp"
    android:paddingLeft="0dp"
    android:paddingTop="0dp"
    android:paddingBottom="0dp"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/bawah"
        android:layout_width="447dp"
        android:layout_height="574dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-22dp"
        android:layout_marginTop="311dp"
        android:layout_marginEnd="-29dp"
        android:layout_marginBottom="-146dp"
        app:srcCompat="@drawable/bawah" />

    <ImageView
        android:id="@+id/botol"
        android:layout_width="356dp"
        android:layout_height="709dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="-321dp"
        app:srcCompat="@drawable/produk" />


    <ImageView
        android:id="@+id/logo"
        android:layout_width="659dp"
        android:layout_height="816dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-102dp"
        android:layout_marginTop="-184dp"
        android:layout_marginEnd="-153dp"
        android:layout_marginBottom="116dp"
        app:srcCompat="@drawable/logogabung" />

    <ImageView
        android:id="@+id/kebaikan"
        android:layout_width="209dp"
        android:layout_height="94dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="92dp"
        android:layout_marginTop="239dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="403dp"
        app:srcCompat="@drawable/tulisan" />

    <ImageView
        android:id="@+id/terpercaya"
        android:layout_width="147dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="392dp"
        app:srcCompat="@drawable/tulisanbawah" />

    <ImageButton
        android:id="@+id/keloginreg"
        android:layout_width="86dp"
        android:layout_height="61dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="164dp"
        android:layout_marginBottom="318dp"
        android:background="@drawable/tombol"
        app:srcCompat="@drawable/tombol" />


</RelativeLayout>


